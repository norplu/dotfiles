set nocompatible
filetype off

set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

Plugin 'VundleVim/Vundle.vim'

call vundle#end()
filetype plugin indent on
"colorscheme distinguished
"colorscheme afterglow
"colorscheme railscasts
set background=dark
colorscheme hybrid


syntax on

set wildmenu

set backspace=indent,eol,start

set autoindent

set number

set shiftwidth=8
set softtabstop=8
set expandtab

set noswapfile

set nobackup

set nobackup
set nowritebackup

set spelllang=en

set spell
